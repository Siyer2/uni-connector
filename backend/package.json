{
  "name": "express-starter-typescript-tsc",
  "main": "src/app.js",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "aws-sdk": "^2.1234.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "nodemon": "^2.0.12",
    "supertest": "^6.2.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "client:start": "nodemon",
    "start": "npm run build && node build/app.local.js",
    "build": "tsc && npm run build-dependencies",
    "build-dependencies": "cp package*.json build/ && npm i --only=prod --prefix=build",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    }
  }
}
